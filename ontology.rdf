    <!-- http://www.co-ode.org/ontologies/ont.owl#AccessRoad -->


    <Class rdf:about="http://www.co-ode.org/ontologies/ont.owl#AccessRoad">
        <equivalentClass>
            <Class>
                <intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.co-ode.org/ontologies/ont.owl#GymnasiumEnvironmentalAspect"/>
                    <Class>
                        <oneOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://www.co-ode.org/ontologies/ont.owl#DirectAccess"/>
                            <rdf:Description rdf:about="http://www.co-ode.org/ontologies/ont.owl#HandicappedAccess"/>
                            <rdf:Description rdf:about="http://www.co-ode.org/ontologies/ont.owl#IndirectAccess"/>
                        </oneOf>
                    </Class>
                </intersectionOf>
            </Class>
        </equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.co-ode.org/ontologies/ont.owl#GymnasiumEnvironmentalAspect"/>
    </Class>
    


    <!-- http://www.co-ode.org/ontologies/ont.owl#AccessibilityFeature -->


    <Class rdf:about="http://www.co-ode.org/ontologies/ont.owl#AccessibilityFeature">
        <rdfs:subClassOf rdf:resource="http://www.co-ode.org/ontologies/ont.owl#GymnasiumFeature"/>
        <rdfs:comment xml:lang="en">All components of the gymnasium should be easily accessible</rdfs:comment>
    </Class>
    


    <!-- http://www.co-ode.org/ontologies/ont.owl#AcousticFeature -->


    <Class rdf:about="http://www.co-ode.org/ontologies/ont.owl#AcousticFeature">
        <rdfs:subClassOf rdf:resource="http://www.co-ode.org/ontologies/ont.owl#GymnasiumFeature"/>
        <rdfs:comment xml:lang="en">The right acoustic configuration need to be worked out during the design phase</rdfs:comment>
    </Class>
    


    <!-- http://www.co-ode.org/ontologies/ont.owl#AfterSchoolActivitiesUse -->


    <Class rdf:about="http://www.co-ode.org/ontologies/ont.owl#AfterSchoolActivitiesUse">
        <rdfs:subClassOf rdf:resource="http://www.co-ode.org/ontologies/ont.owl#HighSchoolUse"/>
        <rdfs:subClassOf>
            <Restriction>
                <onProperty rdf:resource="http://www.co-ode.org/ontologies/ont.owl#hasRequirement"/>
                <someValuesFrom rdf:resource="http://www.co-ode.org/ontologies/ont.owl#GymnasiumEquipmentElement"/>
            </Restriction>
        </rdfs:subClassOf>
    </Class>
    


    <!-- http://www.co-ode.org/ontologies/ont.owl#ArtificialLightFeature -->


    <Class rdf:about="http://www.co-ode.org/ontologies/ont.owl#ArtificialLightFeature">
        <rdfs:subClassOf rdf:resource="http://www.co-ode.org/ontologies/ont.owl#LightFeature"/>
        <disjointWith rdf:resource="http://www.co-ode.org/ontologies/ont.owl#NaturalLightFeature"/>
        <rdfs:comment xml:lang="en">Artificial light sources need to be placed around the gym for adequate lighting and for night time use.</rdfs:comment>
